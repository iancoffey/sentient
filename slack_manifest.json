{
    "display_information": {
        "name": "Sentient"
    },
    "features": {
        "bot_user": {
            "display_name": "Sentient",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "canvases:write",
                "channels:history",
                "channels:read",
                "im:history",
                "pins:read",
                "reactions:read",
                "chat:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://internal",
            "bot_events": [
                "app_mention",
                "message.im",
                "pin_added",
                "pin_removed",
                "reaction_added"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}
